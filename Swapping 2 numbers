#include<stdio.h>  
 int main()    
{    
    int x, y;      
    
    printf("Enter two space-separated numbers: ");
    scanf("%d%d", &x, &y) ;
    
    printf("before swapping\n First number= %d\n Second number= %d\n", x, y);
    
    
    x=x+y;   
    y=x-y;     
    x=x-y;     


    printf("After swapping\n First number= %d\n Second number= %d\n", x, y);    
 
    
    return 0;  
}   
